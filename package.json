{
  "name": "@eliware/discord-template",
  "type": "module",
  "version": "1.1.0",
  "description": "A modern Discord app built with Node.js, based on the eliware/discord foundation.",
  "main": "discord-template.mjs",
  "scripts": {
    "start": "node discord-template.mjs",
    "test": "npx --node-options=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliware/discord-template.git"
  },
  "keywords": [
    "discord-template",
    "discord",
    "discord-bot",
    "discord-app",
    "nodejs"
  ],
  "author": "Eli Sterling, eliware.org <eli@eliware.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eliware/discord-template/issues"
  },
  "homepage": "https://github.com/eliware/discord-template#readme",
  "devDependencies": {
    "jest": "^30.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/?(*.)+(spec|test).mjs",
      "**/?(*.)+(spec|test).cjs"
    ]
  },
  "dependencies": {
    "@eliware/common": "^1.1.1",
    "@eliware/discord": "^1.1.1",
    "dotenv": "^17.2.3"
  }
}
